generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id                   String       @id @db.Uuid
  email               String       @unique
  apiKey             String       @unique
  createdAt                 DateTime     @default(now())
}

model requests {
  id                  String       @id @db.Uuid
  userId          String
  apiKey          String
  createdAt         DateTime     @default(now())
  url                 String
  method          String
  headers         String
  payload          String
}